{
    "name": "zendframework/zendframework1",
    "description": "Zend Framework 1",
    "type": "library",
    "keywords": [
        "framework",
        "zf1"
    ],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.2.11",
	"zend/acl": "dev-master",
            "zf1/zend-loader": "dev-master",
            "zf1/zend-uri": "dev-master",
            "zf1/zend-controller": "dev-master",
            "zf1/zend-exception": "dev-master",
	"zf1/zend-application": "dev-master",
	"zf1/zend-cache": "dev-master",
            "zf1/zend-xml": "dev-master",
	"zf1/zend-config": "dev-master",
            "zf1/zend-locale": "dev-master",
            "zf1/zend-date": "dev-master",
 	"zf1/zend-db": "dev-master",
	"zf1/zend-debug": "dev-master",
            "zf1/zend-validate": "dev-master",
            "zf1/zend-filter": "dev-master",
	"zf1/zend-form": "dev-master",
	"zf1/zend-layout": "dev-master",
	"zf1/zend-log": "dev-master",
            "zf1/zend-mime": "dev-master",
	"zf1/zend-mail": "dev-master",
            "zf1/zend-registry": "dev-master",
	"zf1/zend-measure": "dev-master",
            "zf1/zend-server": "dev-master",
            "zf1/zend-json": "dev-master",
	"zf1/zend-paginator": "dev-master",
	"zf1/zend-session": "dev-master",
	"zf1/zend-version": "dev-master",
	"zf1/zend-view": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Zend_": "library/"
        }
    },
    "include-path": [
        "library/"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.12.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/dbunit": "1.3.*"
    },
    "archive": {
        "exclude": ["/demos", "/documentation", "/tests"]
    }
}
